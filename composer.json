{
    "name": "hexlet/code",
    "autoload": {
        "psr-4": {
            "Hexlet\\Code\\": "src/",
            "Hexlet\\Code\\Tests\\": "tests/"
        },
        "files": [
          "src/Differ.php",
          "src/Parsers.php"
        ]
    },
    "authors": [
        {
            "name": "Vladimir Bun",
            "email": "qwelp@mail.ru"
        }
    ],
    "scripts": {
        "test": "phpunit tests"
    },
    "bin": [
        "bin/gendiff"
    ],
    "require": {
        "docopt/docopt": "^1.0",
        "tightenco/collect": "^5.6",
        "nesbot/carbon": "^2.32.2",
        "symfony/string": "^5.4.2",
        "symfony/yaml": "^6.0",
        "lstrojny/functional-php": "^1.12",
        "hexlet/trees": "^1.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.1",
        "symfony/var-dumper": "^4.0",
        "phpunit/phpunit": "^9.1.3",
        "squizlabs/php_codesniffer": "^3.5.5"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
